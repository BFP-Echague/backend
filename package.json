{
  "name": "be-backend",
  "version": "0.1.0",
  "description": " Backend",
  "main": "index.js",
  "scripts": {
    "test:docker-up": "docker compose -f \"tests/environment/docker-compose.yml\" up -d",
    "test:docker-down": "docker compose -f \"tests/environment/docker-compose.yml\" down",
    "test:migrate-to-image": "dotenv -e \"tests/environment/.env.tests\" -- npx prisma migrate deploy",
    "test:jest-run": "dotenv -e \"tests/environment/.env.tests\" -- jest -i",
    "test:init": "npm run test:docker-up && npm run test:migrate-to-image && npm run test:jest-run",
    "test:finish": "npm run test:docker-down",
    "test:restart": "npm run test:finish && npm run test:init",
    "prisma:migrate": "npx prisma migrate dev",
    "run:docker-up": "docker compose up -d",
    "run:docker-down": "docker compose down",
    "run:server": "node .",
    "run:all": "npm run run:docker-up run:server",
    "start": "ts-node -r tsconfig-paths/register src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BFP-Echague/backend"
  },
  "keywords": [
    "bfp",
    "fire map",
    "backend"
  ],
  "author": "Azelynn Andrew Ritua",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BFP-Echague/backend/issues"
  },
  "homepage": "https://github.com/BFP-Echague/backend#readme",
  "dependencies": {
    "@absxn/process-env-parser": "^1.1.1",
    "@jest/globals": "^29.7.0",
    "@prisma/client": "^6.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.14.9",
    "@types/pdfmake": "^0.2.9",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-list-routes": "^1.2.2",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.1.0",
    "http-status-codes": "^2.3.0",
